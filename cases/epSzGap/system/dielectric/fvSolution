FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/dielectric";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(ep0|ep1|epA0|epA1|epS0|epS1)"
    {
        solver           PCG;
        preconditioner   DIC;
        tolerance        1e-08;
        relTol           0;
    };
}

